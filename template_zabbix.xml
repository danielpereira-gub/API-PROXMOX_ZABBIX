<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.2</version>
    <date>2020-12-27T04:49:20Z</date>
    <groups>
        <group>
            <name>TEMPLATES_SERVIDORES</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>TEMPLATE API_PROXMOX</template>
            <name>TEMPLATE API_PROXMOX</name>
            <groups>
                <group>
                    <name>TEMPLATES_SERVIDORES</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>JSON</name>
                </application>
                <application>
                    <name>PROXMOX</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>JSON: DISCOS DO SERVIDOR</name>
                    <type>HTTP_AGENT</type>
                    <key>json.disk.server</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>JSON</name>
                        </application>
                    </applications>
                    <timeout>15s</timeout>
                    <url>https://{$IP}:8006/api2/json/nodes/{$NOME}/storage</url>
                    <headers>
                        <header>
                            <name>Cookie</name>
                            <value>PVEAuthCookie={$TICKET2}</value>
                        </header>
                    </headers>
                </item>
                <item>
                    <name>JSON NODE</name>
                    <type>HTTP_AGENT</type>
                    <key>json.node</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>JSON</name>
                        </application>
                    </applications>
                    <timeout>5s</timeout>
                    <url>https://{$IP}:8006/api2/json/nodes</url>
                    <headers>
                        <header>
                            <name>Cookie</name>
                            <value>PVEAuthCookie={$TICKET2}</value>
                        </header>
                    </headers>
                </item>
                <item>
                    <name>JSON SERVIDORES</name>
                    <type>HTTP_AGENT</type>
                    <key>json.servidores</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>JSON</name>
                        </application>
                    </applications>
                    <timeout>15s</timeout>
                    <url>https://{$IP}:8006/api2/json/nodes/{$NOME}/qemu</url>
                    <headers>
                        <header>
                            <name>Cookie</name>
                            <value>PVEAuthCookie={$TICKET2}</value>
                        </header>
                    </headers>
                </item>
                <item>
                    <name>TOTAL DE CPU DO SERVIDOR</name>
                    <type>DEPENDENT</type>
                    <key>total.cpu.proxmox</key>
                    <delay>0</delay>
                    <applications>
                        <application>
                            <name>PROXMOX</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$..maxcpu.first()</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>json.node</key>
                    </master_item>
                </item>
                <item>
                    <name>TOTAL DE MEMORIA DO SERVIDOR</name>
                    <type>DEPENDENT</type>
                    <key>total.memoria.proxmox</key>
                    <delay>0</delay>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>PROXMOX</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$..maxmem.first()</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>json.node</key>
                    </master_item>
                </item>
                <item>
                    <name>MEMORIA EM USO NO SERVIDOR</name>
                    <type>DEPENDENT</type>
                    <key>usada.memoria.proxmox</key>
                    <delay>0</delay>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>PROXMOX</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$..mem.first()</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>json.node</key>
                    </master_item>
                </item>
                <item>
                    <name>UTILIZAÇÃO DE CPU DO SERVIDOR</name>
                    <type>DEPENDENT</type>
                    <key>util.cpu.proxmox</key>
                    <delay>0</delay>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>PROXMOX</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$..cpu.first()</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <parameters>
                                <parameter>100</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>json.node</key>
                    </master_item>
                </item>
                <item>
                    <name>UTILIZAÇÃO DE MEMORIA DO SERVIDOR</name>
                    <type>CALCULATED</type>
                    <key>utilizacao.memoria</key>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <params>100*last(&quot;usada.memoria.proxmox&quot;)/
last(&quot;total.memoria.proxmox&quot;)</params>
                    <applications>
                        <application>
                            <name>PROXMOX</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>DISCOVERY DE DISCOS DO SERVER</name>
                    <type>DEPENDENT</type>
                    <key>discovery.disks</key>
                    <delay>0</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>SERVIDOR: {#NOME} - TOTAL DE DISCO</name>
                            <type>DEPENDENT</type>
                            <key>disco.total.[{#NOME}]</key>
                            <delay>0</delay>
                            <history>1w</history>
                            <trends>60d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>PROXMOX</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.storage== '{#NOME}')].total.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.disk.server</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NOME}: ESPAÇO EM USO</name>
                            <type>DEPENDENT</type>
                            <key>disco.usado.[{#NOME}]</key>
                            <delay>0</delay>
                            <history>1w</history>
                            <trends>60d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>PROXMOX</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.storage== '{#NOME}')].used.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.disk.server</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>SERVIDOR: {#NOME} - UTILIZAÇÃO DE DISCO</name>
                            <type>CALCULATED</type>
                            <key>disco.utilizacao.[{#NOME}]</key>
                            <history>1w</history>
                            <trends>30d</trends>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <params>100*last(&quot;disco.usado.[{#NOME}]&quot;) /
last(&quot;disco.total.[{#NOME}]&quot;)</params>
                            <applications>
                                <application>
                                    <name>PROXMOX</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>json.disk.server</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#NOME}</lld_macro>
                            <path>$.storage</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
                <discovery_rule>
                    <name>DISCOVERY SERVIDORES</name>
                    <type>DEPENDENT</type>
                    <key>discovery.servidores</key>
                    <delay>0</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#NAME}: CONFIGURAÇÃO</name>
                            <type>HTTP_AGENT</type>
                            <key>config.[{#ID}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <timeout>5s</timeout>
                            <url>https://{$IP}:8006/api2/json/nodes/{$NOME}/qemu/{#ID}/config</url>
                            <headers>
                                <header>
                                    <name>Cookie</name>
                                    <value>PVEAuthCookie={$TICKET2}</value>
                                </header>
                            </headers>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: TOTAL DE DISCO</name>
                            <type>DEPENDENT</type>
                            <key>disco.total.[{#ID}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].maxdisk.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: DISCO EM USO</name>
                            <type>DEPENDENT</type>
                            <key>disco.utilizado.[{#ID}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].disk.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: IP</name>
                            <type>DEPENDENT</type>
                            <key>ip.[{#ID}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.description.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>config.[{#ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: TOTAL DE MEMORIA</name>
                            <type>DEPENDENT</type>
                            <key>memoria.total.[{#ID}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].maxmem.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: MEMORIA EM USO</name>
                            <type>DEPENDENT</type>
                            <key>memoria.utilizada.[{#ID}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].mem.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: NUMERO DE CPUS</name>
                            <type>DEPENDENT</type>
                            <key>num.cpus.[{#ID}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].cpus.first()</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: STATUS</name>
                            <type>DEPENDENT</type>
                            <key>status.[{#ID}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].status.first()</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>running</parameter>
                                        <parameter>1</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>stopped</parameter>
                                        <parameter>0</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: UTILIZAÇÃO DE CPU</name>
                            <type>DEPENDENT</type>
                            <key>util.cpus.[{#ID}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.[?(@.name == '{#NAME}')].cpu.first()</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>100</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>json.servidores</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{min(5m)}&gt;65</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{min(5m)}&lt;61</recovery_expression>
                                    <name>USO DE CPU ACIMA DE 65% EM {#NAME}</name>
                                    <priority>AVERAGE</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#NAME}: UTILIZAÇÃO DE MEMORIA</name>
                            <type>CALCULATED</type>
                            <key>utilizacao.memoria.[{#ID}]</key>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <params>100*last(&quot;memoria.utilizada.[{#ID}]&quot;) /
last(&quot;memoria.total.[{#ID}]&quot;)</params>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#NAME}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>json.servidores</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#ID}</lld_macro>
                            <path>$.vmid</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#NAME}</lld_macro>
                            <path>$.name</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$IP}</macro>
                </macro>
                <macro>
                    <macro>{$NOME}</macro>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
